unit MacOSAll;{For CodeWarrior 7.1}{$setc ACCESSOR_CALLS_ARE_FUNCTIONS := TRUE}interface	uses Controls, Dialogs, MacTypes, MacWindows, Quickdraw, TextEdit, ToolUtils;	FUNCTION HiWord(x: LONGINT): INTEGER;		FUNCTION LoWord(x: LONGINT): INTEGER;implementation	FUNCTION HiWord(x: LONGINT): INTEGER;	begin		x := BSR(x, 16);		HiWord := BitAnd(x, $FFFF);	end;		FUNCTION LoWord(x: LONGINT): INTEGER;	begin		LoWord := BitAnd(x, $FFFF);	end;end.